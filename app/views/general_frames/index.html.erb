<h1><%= t('.header', university: @university) %></h1>

<table>
  <tr>
    <th><%= GeneralFrame.human_attribute_name :year %></th>
    <th><%= t('.actions') %></th>
  </tr>

<% @general_frames.each do |general_frame| %>
  <tr>
    <td><%= general_frame.year %></td>
    <td><%= link_to t('.show'), general_frame %> |
        <%= link_to t('.edit'), edit_general_frame_path(general_frame) %> |
        <%= link_to t('.destroy'), general_frame, method: :delete, data: { confirm: t('.destroy_confirmation')} %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to t('.new_general_frame'), new_university_general_frame_path(@university) %>
